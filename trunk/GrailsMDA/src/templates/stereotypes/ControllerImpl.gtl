<%= getPackageName(currentModelElement)? "package ${getPackageName(currentModelElement)}":"" %>

<%= getPackageName(currentModelElement)? "import ${getPackageName(currentModelElement)}.interfaces.controller.${currentModelElement.name}Interface":"" %> 

/**
 * Autogenerated Controller ${currentModelElement.name}
 */

public class ${currentModelElement.name} implements ${currentModelElement.name}Interface {
	
<%
        getAttributes(currentModelElement).each { attribute ->
                def attributeName = attribute.name
                def attributeType = javaType(attribute.type)
				def attributeVisibility = visibility(attribute.visibility)
                
%>	${attributeVisibility} ${attributeType} ${attributeName}
<%
        }
%>
	def index = {
	}
<%
		getMethods(currentModelElement).each { method ->
			def methodName = method.name
			def methodType =  method.getSpecification()?method.getSpecification():"def"
			def methodVisibility = visibility(method.visibility)
			def methodReturn = ""
			def methodParameters = ""
			
			method.getParameter().each{
				it.name!="return"?methodParameters+="${javaType(it.type)} ${it.name}, ":""
				if(it.name=="return" && it.type){ 
					methodReturn="return null"
					methodType=javaType(it.type)
				}
			}
			methodParameters -=", "	
			if(methodType=="def"){
%>	
	 ${methodType} ${methodName} = {
	 }
<%
			} else {
%>	
	${methodVisibility} ${methodType} ${methodName} ($methodParameters){
		${methodReturn}
	}
<%				
			}
		}
%>   
}