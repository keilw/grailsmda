<%= getPackageName(currentModelElement)? "package ${getPackageName(currentModelElement)}.interfaces.controller":"" %>

/**
 * Autogenerated GrailsControllerInterface ${currentModelElement.name}
 */

public interface ${currentModelElement.name}Interface {	
<%
		getMethods(currentModelElement).each { method ->
			def methodName = method.name
			def methodType =  method.getSpecification()?method.getSpecification():"def"
			def methodVisibility = visibility(method.visibility)
			def methodReturn = ""
			def methodParameters = ""
			
			method.getParameter().each{
				it.name!="return"?methodParameters+="${javaType(it.type)} ${it.name}, ":""
				if(it.name=="return" && it.type){ 
					methodReturn="return null"
					methodType=javaType(it.type)
				}
			}
			methodParameters -=", "		
%>	
	${methodVisibility} ${methodType} ${methodName} ($methodParameters)
<%
		}
%>   
}